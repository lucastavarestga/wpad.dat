function FindProxyForURL(url, host) {
 
// Se o hostname estiver abaixo, passe direto sem proxy.
    if (dnsDomainIs(host, "empresa.com.br") ||
        dnsDomainIs(host, "google-analytics.com") ||
        shExpMatch(host, "*.facebook.com") || 
        shExpMatch(host, "200.200.200.1") || 
        shExpMatch(host, "*static.ak.facebook.com")) {
        return "DIRECT";
	}
 
// Se o protocolo ou URL, passe direto sem proxy.
        if (url.substring(0, 4)=="ftp:") {
                return "DIRECT";
        }
 
// Se o site solicitado est√° hospedado dentro da rede interna, passe direto sem proxy.
    if (isPlainHostName(host) ||
        shExpMatch(host, "*.local") ||
        shExpMatch(host, "*.intranet") ||
        isInNet(dnsResolve(host), "172.16.0.0", "255.255.248.0") ||
        isInNet(dnsResolve(host), "192.168.0.0", "255.255.240.0") ||
        isInNet(dnsResolve(host), "127.0.0.0", "255.255.255.0")) {

        return "DIRECT";
	}

//IPs Liberados sem proxy
	if ((myIpAddress()=="192.168.0.254") ||
	   (myIpAddress()=="192.168.0.72") ||
	   (myIpAddress()=="192.168.0.136")) {
		return "DIRECT";
	}

// Definindo Proxy para outra feixa de Rede
    if (isInNet(myIpAddress(), "192.168.6.0", "255.255.255.0")) {
        return "PROXY 192.168.6.254:3128";
	}
 
// Proxy Padrao e proxy reserva
    return "PROXY 192.168.0.254:3128; PROXY 192.168.0.171:3128";
 
}
